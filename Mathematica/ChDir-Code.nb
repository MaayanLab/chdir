Notebook[{
Cell[TextData[{
 "The following function takes two data sets corresponding to the two classes \
as inputs (data1 is a p x ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " matrix and data2 is a p x ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " matrix where p is the number of genes on the array and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "1"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["n", "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " are the number of samples in each class) and also a vector of shrinkage \
values \[Gamma], and outputs a unit p-vector which is the characteristic \
direction."
}], "Text",
 CellChangeTimes->{{3.590850998047005*^9, 3.590851101086899*^9}, {
  3.5908511530288696`*^9, 3.590851192571131*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DEdirRegLDA3", "[", 
    RowBox[{"data1_", ",", "data2_", ",", "\[Gamma]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "Xdata", ",", "shiftdata", ",", "t", ",", "keepPC", ",", "Yclass", ",", 
       "b", ",", "DEdir", ",", "DEdirLogistic", ",", "DEdirLDA", ",", "x", 
       ",", "n1", ",", "n2", ",", " ", "project1", ",", "project2", ",", 
       "project3", ",", " ", "meanvec", ",", "V", ",", "R", ",", "Dd", ",", 
       "\[Sigma]", ",", "ShrunkMats", ",", "HPV"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n1", "=", 
       RowBox[{
        RowBox[{"Dimensions", "[", "data1", "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"n2", "=", 
       RowBox[{
        RowBox[{"Dimensions", "[", "data2", "]"}], "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"Xdata", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", "data1", "]"}], ",", 
         RowBox[{"Transpose", "[", "data2", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"shiftdata", "=", 
       RowBox[{
        RowBox[{"Transpose", "[", "Xdata", "]"}], "-", 
        RowBox[{"Mean", "[", "Xdata", "]"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"t", "=", 
       RowBox[{"findPCs", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", "shiftdata", "]"}], ",", 
         RowBox[{"Min", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Dimensions", "[", "shiftdata", "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}], "-", "1"}], ",", "20"}], "]"}]}],
         "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"keepPC", "=", 
       RowBox[{"LengthWhile", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Accumulate", "[", 
           RowBox[{"t", "[", 
            RowBox[{"[", "3", "]"}], "]"}], "]"}], "/", 
          RowBox[{"Total", "[", 
           RowBox[{"t", "[", 
            RowBox[{"[", "3", "]"}], "]"}], "]"}]}], ",", 
         RowBox[{
          RowBox[{"#", "<", "0.99"}], " ", "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Yclass", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"ConstantArray", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "0"}], "}"}], ",", 
           RowBox[{"Length", "[", 
            RowBox[{"Transpose", "[", "data1", "]"}], "]"}]}], "]"}], ",", 
         RowBox[{"ConstantArray", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "1"}], "}"}], ",", 
           RowBox[{"Length", "[", 
            RowBox[{"Transpose", "[", "data2", "]"}], "]"}]}], "]"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"x", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Join", "[", 
           RowBox[{
            RowBox[{"{", "1", "}"}], ",", "#"}], "]"}], "&"}], ",", 
         RowBox[{"t", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"meanvec", "=", 
       RowBox[{
        RowBox[{"Mean", "[", 
         RowBox[{"Transpose", "[", "data2", "]"}], "]"}], "-", 
        RowBox[{"Mean", "[", 
         RowBox[{"Transpose", "[", "data1", "]"}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"V", "=", 
       RowBox[{"t", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"R", "=", 
       RowBox[{"t", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Dd", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"Transpose", "[", "R", "]"}], ".", "R"}], "/", 
        RowBox[{"(", 
         RowBox[{"n1", "+", "n2"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Dd", "=", 
       RowBox[{"LowerTriangularize", "[", 
        RowBox[{"UpperTriangularize", "[", "Dd", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Sigma]", "=", 
       RowBox[{"Mean", "[", 
        RowBox[{"Diagonal", "[", "Dd", "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"ShrunkMats", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Inverse", "[", 
           RowBox[{
            RowBox[{"#", "*", "Dd"}], "+", 
            RowBox[{"\[Sigma]", 
             RowBox[{"(", 
              RowBox[{"1", "-", "#"}], ")"}], "*", 
             RowBox[{"IdentityMatrix", "[", 
              RowBox[{"Length", "[", "Dd", "]"}], "]"}]}]}], "]"}], " ", 
          "&"}], ",", "\[Gamma]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"b", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"V", ".", "#", ".", 
           RowBox[{"Transpose", "[", "V", "]"}], ".", "meanvec"}], " ", "&"}],
          ",", "ShrunkMats"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"b", "=", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "/", 
           RowBox[{"Norm", "[", "#", "]"}]}], " ", "&"}], ",", "b"}], 
        "]"}]}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
